{% from "./mixins.jinja2" import subtree %}

<script type="text/javascript">
 var setSearchBarOnlySiteResources = true;
</script>

<div class="quick-access-links d-print-none">
  <section class="quick-access-links-container">
    <p>{{ _('Quick access') }}{{_(':')}}</p>
    <ul class="quick-access-links-links">
      {% for link, title, is_ext in quick_links %}
      <li><a href="{{link}}"
             {%- if is_ext is sameas true %}
             target="_blank"
             rel="nofollow noopener noreferrer"
             title="{{ title }} {{ _("- New window") }}"
             class=""
             {% endif -%}
          >
        {{ title }}
        {%- if is_ext is sameas true %}
        <i class="fa fa-external-link" aria-hidden="true"></i>
        {% endif -%}
      </a></li>
      {% endfor %}
    </ul>
  </section>
</div>
<div class="container pro">
  <section class="homepage-pro-container row">
    <div class="col-lg-6 col-xl-8">
      <h1 id="section-gerer-title" class="section-tree-title">{{ _('Manage archives') }}</h1>
      <button id="fold-all-tree" class="d-print-none"
         data-label-collapse="{{_('Fold all') }}"
         data-label-expand="{{_('Unfold all') }}"
         aria-expanded="false" aria-controls="tsd-1">
         {{_('Unfold all') }}
      </button>
      <div class="clearfix"></div>
      <nav id="section-tree" aria-labelledby="section-gerer-title">
        {{ subtree(sections, 1, 1) }}
      </nav>
    </div>
    <div class="col-md-8 col-lg-6 col-xl-4 aside-column-cards">
      <h1>{{ _('On the front page') }}</h1>
      {% from "./mixins.jinja2" import on_front_page %}
      {{Â on_front_page(entities, _) }}
    </div>
  </section>
</div>
{% include "bottom-links.jinja2" %}
